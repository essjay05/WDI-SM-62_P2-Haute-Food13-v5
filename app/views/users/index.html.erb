<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<p id="notice"><%= notice %></p>

<h1>Posts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |post| %>
      <tr>
        <td><%= link_to 'Show', post %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<h4><a href="/users/<%= current_user.id %>/posts/new"> New Post </a></h4>